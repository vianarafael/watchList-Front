ry.push(\"/dashboard\")\n    }\n    \n\n  }\n\n  useEffect( () => {\n    const getResponse = async () => {\n\n    };\n    getResponse();\n  }, [])\n\n\n  const history = useHistory();\n  return (\n    <>\n    {localStorage.token ? history.push(\"/dashboard\"): (\n      <div className=\"RegistrationLogin\">\n      <Registration>\n        <h1>Registration</h1>\n        <label>Username</label>\n        <input type=\"text\" onChange={e => {\n          setUsernameReg(e.target.value);\n        }} />\n        <label>Password</label>\n        <input type=\"text\" onChange={e => {\n          setPasswordReg(e.target.value);\n        }} />\n        <button onClick={register}>Register</button>\n      </Registration>\n      <Registration>\n        <h1>Login</h1>\n        <input type=\"text\" placeholder=\"Username...\" onChange={e => {\n          setUsername(e.target.value);\n        }} />\n        <input type=\"password\" placeholder=\"Password ..\" onChange={e => {\n          setPassword(e.target.value);\n        }} />\n        <button onClick={login}>Login</button>\n      </Registration>\n    </div>\n    )\n  \n  }\n  </>\n  );\n}\n\nexport default RegistrationLogin;\n","/Users/rafael/Projects/watchList/client/src/components/Dashboard.js",[],"/Users/rafael/Projects/watchList/client/src/components/Details.js",[],{"ruleId":"64","replacedBy":"65"},{"ruleId":"66","replacedBy":"67"},{"ruleId":"68","severity":1,"message":"69","line":28,"column":12,"nodeType":"70","messageId":"71","endLine":28,"endColumn":17},{"ruleId":"68","severity":1,"message":"72","line":28,"column":19,"nodeType":"70","messageId":"71","endLine":28,"endColumn":27},{"ruleId":"68","severity":1,"message":"73","line":42,"column":10,"nodeType":"70","messageId":"71","endLine":42,"endColumn":25},{"ruleId":"68","severity":1,"message":"74","line":30,"column":10,"nodeType":"70","messageId":"71","endLine":30,"endColumn":21},"no-native-reassign",["75"],"no-negated-in-lhs",["76"],"no-unused-vars","'state' is assigned a value 