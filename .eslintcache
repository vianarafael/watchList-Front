 dispatch] = useReducer(reducer, initialState);\n  return (\n    <SelectedMovieContext.Provider value={{state, dispatch}}>\n      <Router>\n        <Switch>\n          <Route exact path=\"/\" component={Home} />\n          <Route exact path=\"/log\" component={RegistrationLogin} />\n          <Route exact path=\"/dashboard\" component={Dashboard} />\n          <Route exact path=\"/details\" component={Details} />\n        </Switch>\n      </Router>\n    </SelectedMovieContext.Provider>\n  )\n}\n\nexport default App;","/Users/rafael/Projects/watchList/client/src/components/Home.js",[],"/Users/rafael/Projects/watchList/client/src/components/Carrousel/index.js",["81","82"],"import React, { useState, useEffect } from 'react';\nimport { VideoCardGroupContainer, Title } from './styles';\nimport VideoCard from './components/VideoCard';\nimport Slider, { SliderItem } from './components/Slider';\n\nimport axios from 'axios';\n\nfunction Carousel({ color, title, films, fromDashboard }) {\n  const categoryTitle = title;\n  const categoryColor = color;\n  // const [genres, setGenres] = useState([]);\n  // // hacky - removing the film that has no videos\n  // if (films) {\n  //   films = films.filter((film) => film.id !== 19404);\n  // }\n\n  // useEffect(() => {\n  //   fetch(\n  //     'https://api.themoviedb.org/3/genre/movie/list?api_key=e576111d75dee905a12167d6f1387f71&language=en-US'\n  //   )\n  //     .then((res) => res.json())\n  //     .then((res) => setGenres(res.genres));\n  // }, [setGenres]);\n\n  // const genreConverter = {};\n  // if (genres) {\n  //   genres.forEach((genre) => {\n  //     genreConverter[genre.id] = genre.name;\n  //   });\n  // }\n\n  return (\n    <VideoCardGroupContainer>\n      {categoryTitle && (\n        // <>\n        <Title style={{ backgroundColor: categoryColor || 'red' }}>\n          {categoryTitle}\n        </Title>\n      )}\n      <Slider>\n        {films\n          ? films.map((film) => (\n              <SliderItem key={film.original_title}>\n                <VideoCard\n                  id={film.id}\n                  videoTitle={film.original_title}\n                  // genres={film.genre_ids.map(\n                  //   (genre_id) => genreConverter[genre_id]\n                  // )}\n                  poster={film.poster_path}\n                  categoryColor={categoryColor}\n                />\n                {fromDashboard ? <i onClick={ async () => {\n                  // remove this film from the database\n                    axios.delete(`/removeMovie/${film.id}`, { data: { uid: localStorage.uid } });\n\n                    // horrible solution - refactor that later\n                    window.location.reload(false);\n                }} className=\"fas fa-trash-alt\"></i> : ''}\n              </SliderItem>\n            ))\n          : ''}\n      </Slider>\n    </VideoCardGroupContainer>\n  );\n}\n\nexport default Carousel;","/Users/rafael/Projects/watchList/client/src/components/Carrousel/components/VideoCard.js",[],"/Users/rafael/Projects/watchList/client/src/components/Carrousel/styles.js",[],"/Users/rafael/Projects/watchList/client/src/components/Carrousel/components/styles.js",[],"/Users/rafael/Projects/watchList/client/src/components/Carrousel/components/Slider.js",["83"],"import React from 'react';\nimport SlickSlider from 'react-slick';\nimport styled from 'styled-components';\n\nconst Container = styled.ul`\n  padding: 0;\n  margin: 0;\n  .slick-prev,\n  .slick-next {\n    z-index: 50;\n    top: 0;\n    bottom: 0;\n    margin: auto;\n    width: 30px;\n    height: 30px;\n    transform: initial;\n    &:before {\n      font-size: 30px;\n    }\n  }\n  .slick-prev {\n    left: 0;\n  }\n  .slick-next {\n    right: 16px;\n  }\n`;\n\nexport const SliderItem = styled.li`\n  margin-right: 16px;\n  img {\n    margin: 16px;\n    width: 298px;\n    height: 197px;\n    object-fit: cover;\n  }\n  &:focus {\n    outline: none;\n  }\n`;\n\nfunction SamplePrevArrow(props) {\n  const { className, style, onClick } = props;\n  return (\n    <div\n      className={className}\n      style={{ ...style, display: 'block', background: 'green' }}\n      onClick={onClick}\n    />\n  );\n}\n\nconst Slider = ({ children, noRepeat }) => {\n  let infinite;\n  if (noRepeat) {\n    infinite = false;\n  } else {\n    infinite = true;\n  }\n  return (\n    <Container>\n      <SlickSlider\n        {...{\n          dots: false,\n          infinite,\n          speed: 300,\n          centerMode: false,\n          variableWidth: true,\n          adaptiveHeight: true,\n        }}\n      >\n        {children}\n      </SlickSlider>\n    </Container>\n  );\n};\n\nexport default Slider;","/Users/rafael/Projects/watchList/client/src/components/PageDefault.js",[],"/Users/rafael/Projects/watchList/client/src/components/Menu.js",["84"],"import styled from 'styled-components'\nimport { useHistory } from \"react-router-dom\";\nimport { useState, useContext } from 'react';\nimport axios from 'axios';\nimport env from 'react-dotenv';\n\nimport { SelectedMovieContext } from '../App'\nimport Dashboard from './Dashboard';\n\nconst TopMenu = styled.nav`\n  background: #000;\n  padding 16px;\n  display: flex;\n  justify-content: space-around;\n`\n\nconst SignButton = styled.button`\n  color: var(--white);\n  background: var(--red);\n  padding: 8px;\n  border-radius 8px;\n  border-color: var(--red);\n  cursor: pointer;\n  &:focus { outline:none };\n`\n\nconst Logo = styled.img`\n  cursor: pointer;\n`\n\nconst DashboardIcon = styled.i`\n  color: white;\n  cursor: pointer;\n`\n\nconst Menu = () => {\n    const history = useHistory();\n    const [query, setQuery] = useState(null);\n\n    const selectedContext = useContext(SelectedMovieContext);\n\n    const searchMovie = async () => {\n      const response = await axios.get(`https://api.themoviedb.org/3/search/movie?api_key=${env.KEY}&language=en-US&query=${query}`);\n      // I need to make this display in the home if when the function is called\n\n      // 1. set 2 states - searched & the array of searched movies - that goes to home\n\n      // 2. if searched is true, display the searched movies on top\n\n      selectedContext.dispatch({type: 'set_searched', payload: true });\n      selectedContext.dispatch({type: 'set_searched_movies' , payload: response.data.results})\n\n      history.push('/');\n    }\n\n    return (\n        <TopMenu>\n        <Logo onClick={() => {\n          history.push('/');\n        }} src=\"https://fontmeme.com/permalink/210224/4b588a1a31011f69495c99671fd7706e.png\" alt=\"netflix-font\" border=\"0\" />\n        <input type=\"text\" placeholder=\"Search..\" onChange={(e) => {\n          setQuery(e.target.value);\n        }} \n\n          onKeyUp={(e) => {\n          if(e.code === \"Enter\") {\n            // make a call searching for that movie\n            searchMovie();\n          }\n        }} />\n\n        { !localStorage.getItem(\"logged\") ?  \n        (<SignButton onClick={() => {\n            history.push('/log')\n        }}\n        >Sign In</SignButton>)\n        : \n        (\n          <>\n          <DashboardIcon onClick={() => {\n            history.push('/dashboard');\n          }} className=\"fas fa-tachometer-alt fa-2x\"></DashboardIcon>\n        <SignButton onClick={() => {\n          localStorage.removeItem(\"token\");\n          localStorage.removeItem(\"uid\");\n          localStorage.removeItem(\"logged\");\n          history.push('/');\n      }}\n      >Sign Out</SignButton>\n      </>)\n      }\n    </TopMenu>\n    )\n}\n\nexport default Menu;","/Users/rafael/Projects/watchList/client/src/components/RegistrationLogin.js",[],"/Users/rafael/Projects/watchList/client/src/components/Dashboard.js",["85"],"import { useEffect, useState } from \"react\";\nimport axios from \"axios\";\nimport env from 'react-dotenv'\nimport Carrousel from './Carrousel/index'\nimport PageDefault from \"./PageDefault\";\nimport { useHistory } from \"react-router-dom\";\n\nconst Dashboard = ({name}) => {\n  const history = useHistory();\n  const [watchList, setWatchList] = useState([]);\n    useEffect(() => {\n      console.log(localStorage)\n      async function getUserData() {\n          const uid = localStorage.getItem(\"uid\")\n          const getMovies = await axios.get(`/movies/${uid}`,  {\n            headers: {\n              \"x-access-token\": localStorage.token\n            }\n          });\n          let moviesData;\n          if (getMovies.data.message === \"Authentication failed\") {\n            localStorage.removeItem(\"token\");\n            localStorage.removeItem(\"uid\");\n            localStorage.removeItem(\"logged\");\n            history.push('/log');\n          }\n          if (getMovies.data.auth !== false) {\n           moviesData = await Promise.all(getMovies.data.rows.map((film) => axios.get(`https://api.themoviedb.org/3/movie/${film.mid}?api_key=${env.KEY}&append_to_response=videos,credits`)\n            )\n          )\n        } else {\n          // redirect to the login page?\n        }\n        setWatchList(moviesData);\n        }\n      getUserData();\n    },[])\n\n    const displayMovies = () => {\n      const watchListMovies = watchList.map(film => film.data)\n      console.log(watchListMovies)\n        return (\n                  <Carrousel  color={\"#FFA500\"}\n                    title=\"Watch List\"\n                    films={watchListMovies}\n                    noRepeat={true}\n                    fromDashboard={true}\n                    />\n                )\n    }\n    return ( \n    <PageDefault>\n      <>\n       {watchList ? displayMovies() : 'loading'}\n      </>\n    </PageDefault>\n    )\n}\nexport default Dashboard;","/Users/rafael/Projects/watchList/client/src/components/Details.js",["86"],"/Users/rafael/Projects/watchList/client/src/components/BannerMain/index.js",["87"],"import React from 'react';\nimport { useHistory } from 'react-router-dom';\nimport VideoIframeResponsive from './components/VideoIframeResponsive';\nimport {\n  BannerMainContainer,\n  ContentAreaContainer,\n  WatchButton,\n} from './styles';\n\nfunction getYouTubeId(youtubeURL) {\n  return youtubeURL.replace(\n    /^.*((youtu.be\\/)|(v\\/)|(\\/u\\/\\w\\/)|(embed\\/)|(watch\\?))\\??v?=?([^#&?]*).*/,\n    '$7'\n  );\n}\n\nexport default function BannerMain({ videoTitle, videoDescription, url }) {\n  const history = useHistory();\n  const youTubeID = getYouTubeId(url);\n  // const youTubeID = url;\n  const bgUrl = `https://img.youtube.com/vi/${youTubeID}/maxresdefault.jpg`;\n\n  return (\n    <BannerMainContainer backgroundImage={bgUrl}>\n      <ContentAreaContainer>\n        <ContentAreaContainer.Item>\n          <ContentAreaContainer.Title>{videoTitle}</ContentAreaContainer.Title>\n\n          <ContentAreaContainer.Description>\n            {videoDescription}\n          </ContentAreaContainer.Description>\n        </ContentAreaContainer.Item>\n\n        <ContentAreaContainer.Item>\n          <VideoIframeResponsive youtubeID={youTubeID} />\n          <WatchButton>\n            <a\n              href={`https://www.youtube.com/embed/${youTubeID}?autoplay=0&mute=1`}\n            >\n              Watch Trailer\n            </a>\n          </WatchButton>\n        </ContentAreaContainer.Item>\n      </ContentAreaContainer>\n    </BannerMainContainer>\n  );\n}","/Users/rafael/Projects/watchList/client/src/components/BannerMain/components/VideoIframeResponsive.js",[],"/Users/rafael/Projects/watchList/client/src/components/BannerMain/components/styles.js",[],"/Users/rafael/Projects/watchList/client/src/components/BannerMain/styles.js",[],{"ruleId":"88","replacedBy":"89"},{"ruleId":"90","replacedBy":"91"},{"ruleId":"92","severity":1,"message":"93","line":20,"column":5,"nodeType":"94","messageId":"95","endLine":21,"endColumn":45},{"ruleId":"92","severity":1,"message":"96","line":22,"column":5,"nodeType":"94","messageId":"97","endLine":23,"endColumn":20},{"ruleId":"98","severity":1,"message":"99","l